    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#Property -->


    <owl:Class rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    


    <!-- http://www.w3.org/2002/07/owl#Class -->


    <owl:Class rdf:about="http://www.w3.org/2002/07/owl#Class"/>
    


    <!-- https://w3id.org/dedl#API -->


    <owl:Class rdf:about="https://w3id.org/dedl#API">
        <rdfs:subClassOf rdf:resource="https://w3id.org/dedl#Interface"/>
        <rdfs:comment xml:lang="en">Application Programming Interface; a subclass of Interface representing a set of protocols allowing different software entities to communicate within a data ecosystem.</rdfs:comment>
        <rdfs:label xml:lang="en">API</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/dedl#AccessControlModel -->


    <owl:Class rdf:about="https://w3id.org/dedl#AccessControlModel">
        <rdfs:comment xml:lang="en">A conceptual framework defining how access to resources is managed, granted, and restricted.</rdfs:comment>
        <rdfs:label xml:lang="en">AccessControlModel</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/dedl#AgreementStorage -->


    <owl:Class rdf:about="https://w3id.org/dedl#AgreementStorage">
        <rdfs:subClassOf rdf:resource="https://w3id.org/dedl#PolicyStorage"/>
        <rdfs:comment xml:lang="en">A subclass of PolicyStorage where concluded contracts are stored, ensuring that agreements between Participants are documented and accessible.</rdfs:comment>
        <rdfs:label xml:lang="en">AgreementStorage</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/dedl#AuditAndLogging -->


    <owl:Class rdf:about="https://w3id.org/dedl#AuditAndLogging">
        <rdfs:subClassOf rdf:resource="https://w3id.org/dedl#TechnicalDataEcosystemElement"/>
        <rdfs:comment xml:lang="en">A component responsible for documenting activities within a data ecosystem to ensure transparency.</rdfs:comment>
        <rdfs:label xml:lang="en">AuditAndLogging</rdfs:label>
    </owl:Class>
    


