        <rdfs:subClassOf rdf:resource="https://w3id.org/dedl#Process"/>
        <rdfs:comment xml:lang="en">A procedure or sequence of steps that verify the identity of a user or entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Authentication</rdfs:label>
    </owl:Class>
    


