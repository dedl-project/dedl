        <dc:creator xml:lang="en">Jiahang Chen</dc:creator>
        <dc:creator xml:lang="en">Lennart Kl√∂pper</dc:creator>
        <dc:title xml:lang="en">DEDL Vocabulary &amp; Expression 0.1</dc:title>
        <terms:abstract xml:lang="en">The Data Ecosystem Description Language (DEDL) is an abstract language to describe concepts of data ecosystems, in which different participants (i.e., organizations and individuals) share data and generate value as the primary aim.</terms:abstract>
        <terms:license>http://creativecommons.org/licenses/by/4.0/</terms:license>
        <rdfs:comment xml:lang="en">This is a vocabulary for DEDL version 0.1</rdfs:comment>
        <rdfs:label xml:lang="en">DEDL</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.1</owl:versionInfo>
        <schema:affiliation>MMI, RWTH Aachen University</schema:affiliation>
        <schema:email>chen@mmi.rwth-aachen.de</schema:email>
        <schema:email>kloepper@mmi.rwth-aachen.de</schema:email>
    </owl:Ontology>
    


